{
  "kind": "collectionType",
  "collectionName": "tutorials",
  "info": {
    "singularName": "tutorial",
    "pluralName": "tutorials",
    "displayName": "Tutorial",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Update": {
      "type": "date",
      "required": true
    },
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "text",
      "unique": true,
      "required": true
    },
    "Reference": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "Chain": {
      "type": "enumeration",
      "enum": [
        "evm",
        "near",
        "solana",
        "cosmos",
        "polkadot",
        "algorand",
        "icp",
        "cardano",
        "elrond",
        "oasis",
        "ziliqa",
        "kadena",
        "flow",
        "starknet",
        "tezos",
        "iota",
        "stellar"
      ],
      "default": "evm"
    },
    "Source": {
      "type": "enumeration",
      "enum": [
        "medium",
        "github",
        "blog",
        "web",
        "youtube",
        "devto",
        "hashnode",
        "chainlink",
        "twitter"
      ],
      "default": "medium",
      "required": true
    },
    "Series": {
      "type": "string"
    },
    "Tool": {
      "type": "string"
    },
    "Language": {
      "type": "string"
    },
    "Subcategory": {
      "type": "string"
    },
    "Category": {
      "type": "string"
    },
    "Difficulty": {
      "type": "enumeration",
      "enum": [
        "basic",
        "intermediate",
        "advanced",
        "trickster",
        "hacker",
        "expert"
      ],
      "required": true,
      "default": "basic"
    },
    "ViewCounter": {
      "type": "integer",
      "default": 0,
      "min": 0
    }
  }
}
